{
    "name": "dotherightthing/wpdtrt-gallery",
    "description": "Gallery viewer which supports images, panoramas, maps, SoundCloud and Vimeo.",
    "type": "library",
    "keywords": ["gallery", "video", "audio", "map"],
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Smith",
            "email": "dev@dotherightthing.co.nz",
            "homepage": "https://github.com/dotherightthing/wpdtrt-gallery",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dotherightthing.co.nz",
        "issues": "https://github.com/dotherightthing/wpdtrt-gallery/issues"
    },
    "require": {
        "dotherightthing/wpdtrt-plugin-boilerplate": "^1.4.38"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.9.0",
        "phpunit/phpunit": "^5.7",
        "psy/psysh" : "~0.6",
        "wp-cli/wp-cli" : "~0.22",
        "dotherightthing/wpdtrt-contentsections": "^0.1.4",
        "wpackagist-plugin/better-anchor-links": "1.7.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-plugin-boilerplate"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-debug"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-contentsections"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/tgm-plugin-activation"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    }
}
