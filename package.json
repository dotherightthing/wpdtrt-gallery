  "description": "Gallery viewer which supports images, panoramas, maps, SoundCloud and Vimeo.",
  "main": "wpdtrt-gallery.php",
  "person": {
    "name": "Dan Smith",
    "email": "dev@dotherightthing.co.nz",
    "url": "https://profiles.wordpress.org/dotherightthingnz"
  },
  "contributors": [
    {
      "name": "Dan Smith",
